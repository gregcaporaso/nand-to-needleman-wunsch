class DNASequence {

    field String sequence;
    field int length;

    constructor DNASequence new(String s) {
        let length = s.length();
        let sequence = s;

        return this;
    }

    method String getSequenceString() {
        return sequence;
    }

    method int getLength() {
        return length;
    }

    method void print() {
        do Output.printString(sequence);

        return;
    }

}